import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.println("Sayi giriniz");
        int a = scan.nextInt();
        if(a>=100){
            for (int i = a; i > 0; i--){
                System.out.println("i="+i);
            }
        }
        else{
            System.out.println("hatali sayi");
        }

    }
}
